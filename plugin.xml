<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.exbin.bined.eclipse.plugin.editors.BinEdEditor"
            extensions="bin"
            icon="icons/icon.png"
            id="org.exbin.bined.eclipse.plugin.editors.BinEdEditor"
            name="BinEd Binary/Hexadecimal Editor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            name="Sample Wizards"
            id="org.exbin.bined.eclipse">
      </category>
      <wizard
            name="Multi-page Editor file"
            icon="icons/sample.gif"
            category="org.exbin.bined.eclipse"
            class="org.exbin.bined.eclipse.wizards.SampleNewWizard"
            id="org.exbin.bined.eclipse.wizards.SampleNewWizard">
      </wizard>
   </extension>

</plugin>
